###### construct the sphere    #####
####################################


####################################
########      P    ############
####################################
/gate/multiPatchPET/daughters/name container
/gate/multiPatchPET/daughters/insert sphere
/gate/container/setMaterial Air
/gate/container/geometry/setRmax   17.9 cm
/gate/container/geometry/setRmin   14.1 cm               
/gate/container/geometry/setThetaStart 30.0 degree     
/gate/container/geometry/setDeltaTheta 180.0 degree    
/gate/container/vis/forceWireframe                     
/gate/container/vis/setColor gray                      
#/gate/container/vis/setVisible false  
/gate/systems/multiPatchPET/container/attach container            



/gate/container/daughters/name P1
/gate/container/daughters/insert patch
/gate/P1/geometry/setPathToPatchFile Patch_outer170inner150/P1.pat
/gate/P1/setMaterial LYSO
/gate/P1/vis/forceSolid
/gate/P1/vis/setColor yellow
/gate/P1/repeaters/insert ring
/gate/P1/ring/setRepeatNumber 5
/gate/systems/multiPatchPET/Patch1/attach P1
